<Application x:Class="snakegame.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:snakegame"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#3855cd</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#4e68d2</SolidColorBrush>
        <SolidColorBrush x:Key="GrideLineColor">#7a8edd</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#E1DCF2</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#E0E0E0</SolidColorBrush>
        <FontFamily x:Key="MainFont">SnakeAssets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
